load "footprint.5c"
import Footprint;

real pad_width = 0.63;
real pad_height = 0.83;

real package_width = 1.7;
real package_height = 1.25;

real pad_x_off = 1.6 / 2 + pad_width/2;

element_start("sod323");

real dot_off = -.3;

line(package_width/2 + dot_off, -package_height/2,
     package_width/2 + dot_off, package_height/2);

pad_mm(-pad_x_off,
       0,
       pad_width,
       pad_height,
       "1", "1");

pad_mm(pad_x_off,
       0,
       pad_width,
       pad_height,
       "2", "2");

element_end();
