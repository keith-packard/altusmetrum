load "../footprint.5c"
import Footprint;

real hole_space_x = 6.5;
real hole_space_y = 4.5;
real package_width = 6;
real package_height = 6;
real hole_diameter = 1;
real hole_copper = 0.3;

element_start("lph");

for (int y = 0; y < 2; y++) 
	for (int x = 0; x < 2; x++) {
		int	pin = (x + y * 2) + 1;
		string	pin_name = sprintf ("%d", pin);
		string	options = pin == 1 ? "square" : "";
		pin_mm_options (x * hole_space_x - hole_space_x/2, y * hole_space_y - hole_space_y/2, hole_diameter,
				hole_copper, pin_name, pin_name, options);
	}

rect(-package_width/2, -package_height/2, package_width, package_height);

element_end();

