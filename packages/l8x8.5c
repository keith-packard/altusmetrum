load "footprint.5c"
import Footprint;
element_args();

/* from the data sheet */

real pad_width = 1.8;	/* B */
real pad_height = 7.5;	/* C */
real pad_space = 5.6;	/* A */

element_start("l8x8");

pad_mm(-pad_space/2, 0, pad_width, pad_height, "1", "1");
pad_mm( pad_space/2, 0, pad_width, pad_height, "2", "2");

rect(-4, -4, 8, 8);

element_end();
