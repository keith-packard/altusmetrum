load "footprint.5c"
import Footprint;

real hgap	= 0.5;
real vgap	= 0.35;
real width	= 0.6;
real height	= 0.5;

real hpos	= hgap/2 + width/2;
real vpos	= vgap/2 + height/2;

element_start("ABM12");

pad_mm(-hpos, vpos,width,height,"1","1");
pad_mm( hpos, vpos,width,height,"2","2");
pad_mm( hpos,-vpos,width,height,"3","3");
pad_mm(-hpos,-vpos,width,height,"4","4");

element_end();
