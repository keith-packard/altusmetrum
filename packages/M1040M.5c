load "footprint.5c"
import Footprint;

real package_width = 11.7;
real package_height = 11.0;

real pad_width = (12.192 - 5.842) / 2;
real pad_height = 3.254;
real pad_space = 5.842 + pad_width;

element_start("M1040M");

pad_mm(-pad_space/2, 0, pad_width, pad_height, "1", "1");
pad_mm(pad_space/2, 0, pad_width, pad_height, "2", "2");

rect(-package_width/2, -package_height/2, package_width, package_height);

element_end();
