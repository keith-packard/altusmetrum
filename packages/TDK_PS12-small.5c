/* TDK PS12 beeper with minimal size pins */

load "footprint.5c"
import Footprint;

real	body_diam = 12.2;
real	pin_spacing = 5.00;
real	pin_drill = 0.70;

element_start("TDK_PS12");

pin_mm(-pin_spacing/2, 0, pin_drill, process_ring, "1", "1");

pin_mm( pin_spacing/2, 0, pin_drill, process_ring, "2", "2");

arc(0, 0, body_diam/2, body_diam/2, 0, 360);

element_end();



