load "footprint.5c"
import Footprint;

real package_width = 4.75;
real package_height = 3.95;

real pad_width = 2.50;
real pad_height = 2.20;
real pad_space = 4.70;

element_start("DO-214AA");

pad_mm(-pad_space/2, 0, pad_width, pad_height, "1", "1");
pad_mm(pad_space/2, 0, pad_width, pad_height, "2", "2");

rect(-package_width/2, -package_height/2, package_width, package_height);
line(0.8*package_width, -package_height/2, 0.8*package_width, package_height/2);

element_end();
